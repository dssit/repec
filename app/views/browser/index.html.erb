<pre>
<% if @path == "repec" %>
<%= link_to '[To Parent Directory]', "/" %><br>
<%= dir_entry(Time.now, 'cda', '/repec/cda') %>
<% elsif @path == "repec/cda" %>
<%= link_to '[To Parent Directory]', "/repec" %><br>
<%= dir_entry(File.mtime(Rails.root.join('public', 'cdaarch.redif')), 'cdaarch.redif', asset_path("cdaarch.redif"), File.size(Rails.root.join('public', 'cdaarch.redif'))) %>
<%= dir_entry(File.mtime(Rails.root.join('public', 'cdaseri.redif')), 'cdaseri.redif', asset_path("cdaseri.redif"), File.size(Rails.root.join('public', 'cdaseri.redif'))) %>
<%= dir_entry(Time.now, 'wpaper', "/repec/cda/wpaper") %><br>
<% elsif @path == "repec/cda/wpaper" %>
<%= link_to '[To Parent Directory]', "/repec/cda" %><br>
<% @papers.each do |paper| %>
<% cache paper do %><%= dir_entry(paper.created_at.in_time_zone('Pacific Time (US & Canada)'), "#{paper.paper_number}.redif", paper_redif_path(paper), PapersController.render('browser/show.redif', assigns: { paper: paper }).length ) %><% end %>
<% end %>
<% else %>
<%= dir_entry(Time.now, 'repec', '/repec') %>
<% end %>
</pre>